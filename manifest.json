{
    "name": "Pruuus",
    "description": "Utilities in Forms",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": "images/crow.png"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/app.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "match_origin_as_fallback": true
        }
    ],
    "background": {
        "js": [
            "scripts/worker/background.js"
        ]
    },
    "permissions": [
        "notifications"
    ],
    "icons": {
        // "16": "images/crow.png",
        // "32": "images/crow-3.png",
        "48": "images/crow.png"
        // "128": "images/crow.png"
    }
}